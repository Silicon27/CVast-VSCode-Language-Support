{
  "name": "cvast-language-support",
  "displayName": "cvast-language-support",
  "description": "Support for the CVast Programming Languge",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.98.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "iconThemes": [
      {
        "id": "cvast-icons",
        "label": "CVast Icons",
        "path": "./icons/cvast-icon-theme.json"
      }
    ],
    "languages": [{
      "id": "cvast",
      "aliases": ["CVast", "cvast"],
      "extensions": [".cv",".cvast"],
      "configuration": "./language-configuration.json",
      "icon": {
        "light": "./icons/cvast-file-icon.svg",
        "dark": "./icons/cvast-file-icon.svg"
      }
    }],
    "grammars": [{
      "language": "cvast",
      "scopeName": "source.cvast",
      "path": "./syntaxes/cvast.tmLanguage.json"
    }]
  }
}
